<!DOCTYPE html>
<html>

<head>
    <title>THÊM THÔNG TIN</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <h1>THÊM</h1>
    <a href="/form-add"><button>THÊM</button></a>
    <table border="1">
        <tr>
            <th>IdDistrict</th>
            <th>IdVillage</th>
            <th>Address</th>
            <th>BankName</th>
            <th>BranchName</th>
            <th>ACTION</th>
        </tr>
        <% Custommer4s.forEach(item => { %>
            <tr>
                <td>
                    <%= item._id %>
                </td>
                <td>
                    <%= item.IdDistrict %>
                </td>
                <td>
                    <%= item.IdVillage %>
                </td>
                <td>
                    <%= item.Address %>
                </td>
                <td>
                    <%= item.BankName %>
                </td>
                <td>
                    <%= item.BranchName %>
                </td>
                <td>
                    <a href="/form-update/<%= item._id %>">UPDATE</a>||<a href="/form-delete/<%= item._id %>"> DELETE</a>||
                </td>
            </tr>
            <% }) %>
    </table>

</body>

</html>